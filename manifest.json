{
  "manifest_version": 2,

  "name": "No zoom block",
  "description": "This extension keeps amazon image zoomer from blocking the inetractions with the",
  "version": "1.0",
  "content_scripts": [
    {
        "matches": ["https://www.amazon.com/*"],
        "js": ["/popup.js"]
    }
],

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Click here!"
  },
  "permissions": [
    "https://www.amazon.com/*"
  ]
}
